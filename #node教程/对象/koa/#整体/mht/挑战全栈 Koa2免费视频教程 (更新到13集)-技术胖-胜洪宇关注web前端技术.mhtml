    if(ctx.url=3D=3D=3D'/' &amp;&amp; ctx.method=3D=3D=3D'GET'){
        //=E6=98=BE=E7=A4=BA=E8=A1=A8=E5=8D=95=E9=A1=B5=E9=9D=A2
        let html=3D`
