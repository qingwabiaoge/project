                &lt;button type=3D"submit"&gt;submit&lt;/button&gt;
            &lt;/form&gt;
        `;
        ctx.body=3Dhtml;
    }else if(ctx.url=3D=3D=3D'/' &amp;&amp; ctx.method=3D=3D=3D'POST'){
        let pastData=3Dawait parsePostData(ctx);
         ctx.body=3DpastData;
    }else{
        ctx.body=3D'&lt;h1&gt;404!&lt;/h1&gt;';
    }

});

function parsePostData(ctx){
    return new Promise((resolve,reject)=3D&gt;{
        try{
            let postdata=3D"";
            ctx.req.on('data',(data)=3D&gt;{
                postdata +=3D data
            })
            ctx.req.addListener("end",function(){
                let parseData =3D parseQueryStr( postdata )
                resolve(parseData);
            })
        }catch(error){
            reject(error);
        }
    });
}


function parseQueryStr(queryStr){
    let queryData=3D{};
    let queryStrList =3D queryStr.split('&amp;');
    console.log(queryStrList);
    for( let [index,queryStr] of queryStrList.entries() ){
        let itemList =3D queryStr.split('=3D');
        console.log(itemList);
        queryData[itemList[0]] =3D decodeURIComponent(itemList[1]);
    }=20
    return queryData
}

